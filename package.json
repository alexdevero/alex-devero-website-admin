    "html-webpack-plugin": "^2.28.0",
    "imports-loader": "^0.7.1",
    "ip": "^1.1.5",
    "json-loader": "^0.5.4",
    "node-sass": "^4.5.2",
    "normalize.css": "^6.0.0",
    "postcss-loader": "^1.3.3",
    "prop-types": "^15.5.6",
    "react": "^15.5.4",
    "react-dom": "^15.5.4",
    "react-redux": "^5.0.4",
    "react-router": "^3.0.5",
    "redux": "^3.6.0",
    "redux-thunk": "^2.2.0",
    "rimraf": "^2.6.1",
    "sass-loader": "^6.0.3",
    "style-loader": "^0.17.0",
    "url-loader": "^0.5.8",
    "webpack": "^1.15.0",
    "yargs": "^7.1.0"
  },
  "devDependencies": {
